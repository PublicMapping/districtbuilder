{
  "name": "district-builder-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check-format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/server/src/main",
    "lint": "eslint 'src/**/*.{ts,tsx,json}'",
    "fix": "eslint 'src/**/*.{ts,tsx,json}' --fix",
    "reindex": "node dist/server/src/commands/reindex",
    "set-topology-size": "node dist/server/src/commands/set-topology-size",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate -n",
    "migration:create": "yarn run typeorm migration:create -n",
    "migration:revert": "yarn run typeorm migration:revert",
    "migration:run": "yarn run typeorm migration:run"
  },
  "dependencies": {
    "@drdgvhbh/postgres-error-codes": "0.0.6",
    "@nestjs-modules/mailer": "1.6.1",
    "@nestjs/common": "8.4.2",
    "@nestjs/core": "8.4.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.2",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/swagger": "5.2.0",
    "@nestjs/terminus": "8.0.6",
    "@nestjs/typeorm": "8.0.3",
    "@nestjsx/crud": "5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "5.0.0-alpha.3",
    "@turf/area": "6.5.0",
    "@turf/bbox": "6.5.0",
    "@turf/length": "6.5.0",
    "@turf/polygon-to-line": "6.5.0",
    "aws-sdk": "2.1093.0",
    "base64url": "3.0.1",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "crypto": "1.0.1",
    "csv-parse": "4.15.3",
    "csv-stringify": "5.5.1",
    "ejs": "3.1.7",
    "form-data": "4.0.0",
    "geojson2shp": "0.5.0",
    "handlebars": "4.7.7",
    "jest-mock": "^28.1.3",
    "lodash": "4.17.21",
    "nestjs-typeorm-paginate": "3.1.3",
    "nodemailer": "6.7.2",
    "object-sizeof": "1.6.3",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "pg": "8.7.3",
    "promise-queue": "2.2.5",
    "pug": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rollbar": "2.24.0",
    "rxjs": "7.5.5",
    "simplify-geojson": "^1.0.5",
    "threads": "1.7.0",
    "topojson-client": "3.1.0",
    "typeorm": "0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.4",
    "@nestjs/schematics": "8.0.8",
    "@nestjs/testing": "8.4.2",
    "@types/bcrypt": "5.0.0",
    "@types/ejs": "3.1.0",
    "@types/express": "4.17.13",
    "@types/express-serve-static-core": "4.17.28",
    "@types/inline-css": "3.0.1",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.180",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.7",
    "@types/nodemailer": "6.4.4",
    "@types/passport-jwt": "3.0.6",
    "@types/promise-queue": "2.2.0",
    "@types/pug": "2.0.6",
    "@types/supertest": "2.0.11",
    "@types/topojson-client": "3.1.0",
    "@types/uuid": "^8.3.4",
    "@types/validator": "13.7.1",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-functional": "4.0.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsdoc": "37.4.0",
    "eslint-plugin-local-rules": "1.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "supertest": "4.0.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
