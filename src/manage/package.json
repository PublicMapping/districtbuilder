    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "globby": "11.0.0",
    "jest": "26.5.3",
    "prettier": "2.5.1",
    "ts-jest": "26.4.1",
    "ts-node": "10.4.0",
    "typescript": "4.3.5"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "manage",
    "plugins": [
      "@oclif/plugin-help"
