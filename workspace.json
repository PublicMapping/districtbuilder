{
  "version": 2,
  "projects": {
    "client": {
      "root": "src/client",
      "sourceRoot": "src/client",
      "projectType": "application",
      "tags": ["scope:client"],
      "targets": {
        "build": {
          "executor": "@nrwl/web:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/src/client",
            "index": "src/client/public/index.html",
            "main": "src/client/src/index.tsx",
            "polyfills": "src/client/src/polyfills.ts",
            "tsConfig": "src/client/tsconfig.app.json",
            "assets": [
              {
                "input": "src/client/public/",
                "glob": "**/*",
                "output": "./"
              }
            ],
            "styles": ["src/client/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "development": {
              "optimization": false
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "client:build:development",
            "port": 3003,
            "host": "0.0.0.0",
            "baseUrl": "http://localhost:3003",
            "publicHost": "http://localhost:3003",
            "hmr": true,
            "proxyConfig": "src/client/setupProxy.conf"
          },
          "configurations": {
            "production": {
              "buildTarget": "client:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["src/client/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/src/client"],
          "options": {
            "jestConfig": "src/client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "server": {
      "root": "src/server",
      "sourceRoot": "src/server/src",
      "projectType": "application",
      "tags": ["scope:server"],
      "targets": {
        "build": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "additionalEntryPoints": [
              {
                "entryName": "worker",
                "entryPath": "src/server/src/worker.ts"
              }
            ],
            "outputPath": "dist/src/server",
            "main": "src/server/src/main.ts",
            "tsConfig": "src/server/tsconfig.app.json",
            "assets": ["src/server/src/templates"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "e2e": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/src/server"],
          "options": {
            "jestConfig": "src/server/test/jest-e2e.config.js"
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "port": 3005,
            "buildTarget": "server:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "rulesdir": ["tools/eslint-rules"],
            "lintFilePatterns": ["src/server/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/src/server"],
          "options": {
            "jestConfig": "src/server/jest.config.js",
            "passWithNoTests": true
          }
        },
        "typeorm": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "src/server",
            "command": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js"
          }
        }
      }
    },
    "shared": {
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "projectType": "library",
      "tags": ["scope:shared"],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared"],
          "options": {
            "jestConfig": "libs/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
