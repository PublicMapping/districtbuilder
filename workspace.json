{
  "version": 2,
  "projects": {
    "server": {
      "root": "src/server",
      "sourceRoot": "src/server/src",
      "projectType": "application",
      "tags": ["scope:server"],
      "targets": {
        "build": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/src/server",
            "main": "src/server/src/main.ts",
            "tsConfig": "src/server/tsconfig.app.json",
            "assets": ["src/server/src/templates"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "src/server/src/environments/environment.ts",
                  "with": "src/server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "server:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["src/server/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/src/server"],
          "options": {
            "jestConfig": "src/server/jest.config.js",
            "passWithNoTests": true
          }
        },
        "typeorm": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "src/server",
            "command": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js"
          }
        }
      }
    },
    "client": {
      "root": "src/client",
      "sourceRoot": "src/client/src",
      "projectType": "application",
      "tags": ["scope:client"],
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/src/client",
            "index": "src/client/src/index.html",
            "main": "src/client/src/main.tsx",
            "polyfills": "src/client/src/polyfills.ts",
            "tsConfig": "src/client/tsconfig.app.json",
            "assets": ["src/client/src/favicon.ico", "src/client/src/assets"],
            "styles": ["src/client/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/client/src/environments/environment.ts",
                  "with": "src/client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "client:build",
            "hmr": true,
            "proxyConfig": "src/client/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "client:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["src/client/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/src/client"],
          "options": {
            "jestConfig": "src/client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared": {
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "projectType": "library",
      "tags": ["scope:shared"],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared"],
          "options": {
            "jestConfig": "libs/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "client"
}
